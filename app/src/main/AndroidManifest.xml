<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sdr.ufscar.dev.srdc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.CadastroCidadaoActivity" />
        <activity android:name=".activity.RegistrosActivity" />
        <activity android:name=".activity.CadastroRegistro" />
        <activity
            android:name=".activity.ConfiguracaoActivity"
            android:label="@string/title_activity_configuracao"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>